<%= form_with url:custom_search_path , method: :get , class: "form-inline" , local: true do |form| %>
   <%= form.search_field :search , class: "form-control mr-sm-2" , placeholder:"Search" , aria:{label:"Search"} %>
   <%= form.submit "Search" , class: "btn btn-outline-success my-2 my-sm-0 mr-2" %>
<% end %>
